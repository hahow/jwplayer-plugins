{
  "name": "@hahow/jwplayer-plugin-full-viewport",
  "version": "0.0.4",
  "description": "This is a JW Player plugin that allows users to switch the video player to full-viewport size mode",
  "keywords": [
    "jwplayer",
    "jwplayer plugin"
  ],
  "homepage": "https://github.com/hahow/jwplayer-plugins/tree/main/packages/jwplayer-plugin-full-viewport",
  "bugs": {
    "url": "https://github.com/hahow/jwplayer-plugins/issues"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "sideEffects": false,
  "files": [
    "dist/**"
  ],
  "devDependencies": {
    "@types/jwplayer": "^8.2.10",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "tsconfig": "*",
    "tsup": "^5.10.1",
    "typescript": "^4.5.2"
  },
  "scripts": {
    "build": "tsup src/index.ts --format esm,cjs --dts && tsup src/fullViewport.ts --format iife --legacy-output",
    "dev": "tsup src/index.ts --format esm,cjs --watch --dts",
    "lint": "eslint src/**/*.ts && tsc --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/hahow/jwplayer-plugins.git",
    "directory": "packages/jwplayer-plugin-full-viewport"
  },
  "publishConfig": {
    "access": "public"
  }
}
